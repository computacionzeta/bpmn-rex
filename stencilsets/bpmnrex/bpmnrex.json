			"icon":			"edge.resourceClassification.png",
			"roles":		["mixedRelationship"]
		}
	],
	"rules":{
		"connectionRules": [
			{
				"role": 	"classifierRelationship",
				"connects": [
					{
						"from": "resourceClassifier",
						"to":	"resourceClassifier"
					}
				],
			},
			{
				"role": 	"mixedRelationship",
				"connects": [
					{
						"from": "humanResource",
						"to":	"resourceClassifier"
					}
				],
			}
		],
		"containmentRules":	[
			{
				"role":		"Diagram",
				"contains": [
							"classification",
							"classifierRelationship",
							"mixedRelationship"
				]
			},
			{
				"role":		"classification",
				"contains": [
							"resourceParameter",
							"resourcePrivilege"
				]
			}
		]
		
	}
