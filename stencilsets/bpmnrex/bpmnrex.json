			"description":	"A reference from a Resource Classifier to another Resource Classifier",
			"view":			"edge.resourceReference.svg",
			"icon":			"edge.resourceReference.png",
			"roles":		["classifierRelationship"]
		},
		{
			"type":			"edge",
			"id":			"subsumption",
			"title":		"Subsumption",	
			"groups":		["Relationship"],
